{
  "settings": {
    "custom_app_feature_enabled": true
  },
  "rules": [
    {
      "patterns": [
        ".+"
      ],
      "properties": {
        "context": "default"
      }
    },
    {
      "patterns": [
        "/new$",
        "/edit$"
      ],
      "properties": {
        "context": "modal"
      }
    }
  ]
}
